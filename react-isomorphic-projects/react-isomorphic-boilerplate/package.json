{
  "name": "isomorphic-demo",
  "version": "0.0.1",
  "description": "A boilerplate for isomorphic React apps",
  "main": "lib/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib --experimental"
  },
  "keywords": [
    "isomorphic",
    "react",
    "app",
    "component",
    "chrome",
    "firefox",
    "ie",
    "detection",
    "detect",
    "browser"
  ],
  "author": "Joseph Furlott",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-core": "^5.5.6",
    "babel-loader": "^5.1.4",
    "express": "^4.12.4",
    "jade": "^1.10.0",
    "node-libs-browser": "^0.5.2",
    "react": "^0.14.7",
    "react-hot-loader": "^1.2.7",
    "react-router": "^0.13.3",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "download-file": "^0.1.5",
    "express-react-views": "^0.10.0",
    "fetch": "^1.0.1",
    "jsdom": "^9.2.1",
    "lets-get-meta": "^1.0.0",
    "material-ui": "^0.14.4",
    "nodemon": "^1.3.7",
    "react": "^0.14.8",
    "react-addons-css-transition-group": "^15.1.0",
    "react-alert": "^1.0.14",
    "react-bootstrap": "^0.29.0",
    "react-dnd": "^2.1.4",
    "react-dom": "^0.14.7",
    "react-engine": "^3.1.0",
    "react-helmet": "^3.1.0",
    "react-tap-event-plugin": "^1.0.0",
    "superagent": "^1.8.3"
  }
}
